# Server
PORT=3000
NODE_ENV=development

# Storage
UPLOADS_DIR=./uploads
TEMP_DIR=./temp
MAX_FILE_SIZE_MB=500

# AssemblyAI
ASSEMBLYAI_API_KEY=your_assemblyai_key_here

# Shotstack
